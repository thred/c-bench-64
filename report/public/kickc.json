    "stdio": "partly",
    "stdlib": "partly",
    "stdnoreturn": "no",
    "string": "yes",
    "tgmath": "no",
    "threads": "no",
    "time": "no",
    "uchar": "no",
    "wchar": "no",
    "wctype": "no"
  },
  "runtimeSupport": [
    "Commodore VIC 20",
